{
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs14.x",
        "maxDuration": 30
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/" }
    ],
    "builds": [
      {
        "src": "src/**/*.ts",
        "use": "@vercel/node"
      }
    ]
  }
  