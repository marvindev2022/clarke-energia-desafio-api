generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Supplier {
  id                   String      @id @default(uuid())
  name                 String
  logo                 String   
  state_abbreviation   String   
  cost_per_kwh         Int
  min_kwh_limit        Int
  total_customers      Int
  average_customer_rating Float
}
